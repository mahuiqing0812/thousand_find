/* 支付弹窗单选按钮样式 */
radio-group {
  font-size: 28rpx;
  color: #333;
}

radio-group label {
  display: block;
  height: 86rpx;
  line-height: 86rpx;
  border-bottom: 2rpx solid #F2F2F2;
  position: relative;
  padding-left: 56rpx;
}

radio-group label image {
  position: absolute;
  left: 0;
  width: 36rpx;
  height: 36rpx;
  top: 24rpx;
}

.cancelOrder_reason .reasons_item {
  height: 88rpx;
  display: block;
  line-height: 88rpx;
  border-bottom: 2rpx solid #F2F2F2;
  box-sizing: border-box;
  position: relative;
}

radio .wx-radio-input {
  border-radius: 50%;
  width: 32rpx;
  height: 32rpx;
  background-color: #fff;
  border: 2px solid #f2f2f2;
  position: absolute;
  right: 24rpx;

}

radio .wx-radio-input.wx-radio-input-checked {
  border: none;
  background-image: linear-gradient(to right, #FFB43C, #FF504C);
}

radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%;
  /* 圆角 */
  width: 28rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 28rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 30rpx;
  text-align: center;
  font-size: 20rpx;
  /* 对勾大小 30rpx */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

/* 确认支付按钮 */
.confirmPay {
  width: 320rpx;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  text-align: center;
  background-image: linear-gradient(to right, #FFB43C, #FF504C);
  margin: 60rpx auto 0;
  border-radius: 68rpx;
}

.notEnoughMoney {
  position: absolute;
  right: 30rpx;
  background-color: #fff;
  z-index: 1;
  margin: 0;
  color: #3f3f3f;
}

radio text {
