/* banner开始 */
page{
  font-size: 24rpx;
  padding-bottom: 98rpx;
}
image{
  width: 100%;
}

.nav-tabs {
  width: 100%;
  display: flex;
  position: fixed;
  bottom: 0;
  border: 2rpx solid #eeeeee;
  font-size: 20rpx;
  background-color: #fff;
  z-index: 222;
}

.tab-list {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column-reverse;
  background: #ffffff;
  height: 100rpx;

}

.tab-text {
  font-size: 24rpx;
  line-height: 35rpx;
}

.iconPath {
  width:54rpx;
  height: 54rpx;
}

.tab-content {
  flex: 1;
}

.default {
  text-align: center;
  flex: 1;
  color: #eee;
  color: #c1c1c1;
  font-size: 20rpx;
}

.active {
  line-height: 75rpx;
  text-align: center;
  flex: 1;
  font-size: 20rpx;
  color: #EE4A50;
}

.show {
  display: block;
  flex: 1;
}

.hidden {
  display: none;
  flex: 1;
}

.kefu{
  position: fixed;
  z-index: 111111;
  bottom: 0;
  right: 0;
  width: 33.33%;
  border: 0;
  height: 100rpx;
  border-radius: 0;
  background-color: transparent;
  padding: 0;
  box-sizing: content-box;
}

.kefu::after{
  border:none;
  box-sizing: content-box;
  background-color: transparent;
  padding: 0;
  border-radius: 0;
}

