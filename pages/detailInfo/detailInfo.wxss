  height: 228rpx;
  box-sizing: border-box;
  padding: 30rpx;
  background-color: #fff;
}

.contact-title {
  margin-bottom: 15rpx;
}

/*与本人联系  */
.contact-self {
  width: 750rpx;
  height: 192rpx;
  background-color: #fff;
}

.contactSelf-phone {
  display: flex;
  height: 88rpx;
  width: 720rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-top: 1rpx solid #D9D9D9;
  border-bottom: 1rpx solid #D9D9D9;
  line-height: 88rpx;
  margin: 0rpx 15rpx;
}

.contactSelf-input {
  width: 430rpx;
  height: 88rpx;
  line-height: 88rpx;
}

