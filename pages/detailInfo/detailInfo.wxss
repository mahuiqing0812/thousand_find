  padding: 12rpx 30rpx;
  font-size: 24rpx;
  color: #D0021B;
}

/*与房东联系  */
.contact-house {
  width: 750rpx;
  height: 245rpx;
  background-color: #fff;
}

/*项目简介  */
.store-item {
  position: relative;
  overflow: auto;
}

.item-textarea {
  width: 750rpx;
  height: 240rpx;
  padding: 30rpx;
  /* border-bottom: 1rpx solid #bebebe; */
  background-color: #fff;
  box-sizing: border-box;
  z-index: 1;
}

/*上传图片  */
.img-num {
  flex-wrap: wrap;
  display: flex;
  padding: 0rpx 20rpx;
  /* border-bottom:1rpx solid #bebebe */
  margin: 0 auto;
}

.upload-img {
  width: 224rpx;
  height: 227rpx;
  background-color: #fff;
  margin: 5rpx;
  position: relative;
  border-radius: 10rpx;
}

.upload-img image {
  width: 216rpx;
  height: 160rpx;
  /* border-radius:10rpx; */
}

.default-img {
  width: 216rpx;
  height: 160rpx;
  box-sizing: border-box;
  /* color:#999; */
  text-align: center;
  font-size: 100rpx;
  margin: 0rpx 10rpx 10rpx;
  background: #FFFFFF;
  box-shadow: 0 2rpx 6rpx -2rpx rgba(0, 0, 0, 0.30);
  border-radius: 16rpx;

}

.default-img image {
  width: 216rpx;
  height: 160rpx;
}

.default-img .img-noImg {
  width: 48rpx;
  height: 48rpx;
  /* margin-top:10rpx; */
  vertical-align: middle;

}

.default-img .img-haveImg {
  border: 0;
  border-radius: 16rpx;
}

/*照片文字提示  */
.img-alt {
  color: #999;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
}

/*保存提交  */
.btn-area {
  height: 200rpx;
  width: 588rpx;
  margin: 0 auto;
  line-height: 200rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 60rpx;
}

.btn-area button {
  width: 260rpx;
  height: 80rpx;
  box-shadow: 0 2rpx 8rpx -2rpx rgba(0, 0, 0, 0.30);
  border-radius: 0;
  font-size: 28rpx;
  -webkit-tap-highlight-color: 0;
  margin: 0;
  padding: 0;
  margin-top: 40rpx;
  color: #1a171c;
  line-height: 80rpx;
}

button::after {
  border: 0;
}

/*保存按钮  */
.btn-area .btn-save {
  color: #fff;
  border: 1rpx solid #c19e7b;
  background: #C19E7B;
}

.btn-area .submit-btn {
  color: #c19e7b;
  background-color: #1a171c;
}

.btn-area .disabled-btn {
  color: #999;
  background: #D9D9D9;

}

.mask {
  width: 750rpx;
  height: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 20000;
}


.operate_btns .cancel_btn,
.operate_btns .order_btn {
  top:12rpx;
}

